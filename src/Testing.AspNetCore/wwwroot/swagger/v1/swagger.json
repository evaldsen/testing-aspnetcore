{
  "openapi": "3.0.1",
  "info": {
    "title": "Testing Demo API",
    "version": "v1"
  },
  "paths": {
    "/Logging/message/{level}": {
      "post": {
        "tags": [
          "Logging"
        ],
        "operationId": "LogMessage",
        "parameters": [
          {
            "name": "level",
            "in": "path",
            "required": true,
            "schema": {
              "enum": [
                "trace",
                "debug",
                "information",
                "warning",
                "error",
                "critical",
                "none"
              ],
              "type": "string",
              "default": ""
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "string"
              }
            },
            "text/json": {
              "schema": {
                "type": "string"
              }
            },
            "application/*+json": {
              "schema": {
                "type": "string"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {}
}